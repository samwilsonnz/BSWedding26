{
  "name": "sam-and-beatrice-wedding-registry",
  "version": "2.2.0",
  "description": "Sam & Beatrice's wedding gift registry with Supabase and Stripe payment integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node migrate-to-supabase.js",
    "backup": "node -e \"require('fs').createReadStream('./registry-data.json').pipe(require('fs').createWriteStream('./backup-' + new Date().toISOString().split('T')[0] + '.json'))\"",
    "restore": "echo 'Copy your backup file to registry-data.json to restore'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "echo 'Creating .env file...' && cp .env.example .env && echo 'Setup complete! Edit .env with your settings.'"
  },
  "keywords": [
    "wedding",
    "registry",
    "gifts",
    "contributions",
    "sam",
    "beatrice",
    "supabase",
    "stripe",
    "payments"
  ],
  "author": "Sam & Beatrice",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^4.0.1",
    "resend": "^6.6.0",
    "stripe": "^14.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "bugs": {
    "url": "mailto:registry@example.com"
  },
  "homepage": "http://localhost:3000"
}
